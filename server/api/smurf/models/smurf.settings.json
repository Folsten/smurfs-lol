    },
    "warranty": {
      "type": "string",
      "default": "warranty",
      "required": true
    },
    "originalPrice": {
      "type": "decimal"
    },
    "resource": {
      "type": "string",
      "required": true
    },
    "level": {
      "type": "string",
      "required": true,
      "default": "30 or higher"
    },
    "rank": {
      "type": "string",
      "required": true
    },
    "server": {
      "via": "smurfs",
